@model StudentsIndexViewmodel

<h1>Students</h1>
<table class="table table-hover table-responsive-md">
    <tr>
        <th>Name</th>
        <th>Action</th>
    </tr>
    @foreach(var student in Model.Students)
    { 
        <tr>
            <td>@student.StudentName</td>
            <td><a asp-controller="StudentList"
                   asp-action="Add"
                   asp-route-id="@student.Id"
                   class="btn btn-outline-info">Add to session list</a></td>
        </tr>
    }
</table>

